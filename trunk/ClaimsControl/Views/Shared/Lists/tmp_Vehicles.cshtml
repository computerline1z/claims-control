		{{#if App.listAllController.endDate}}
					<div style="background-color:#f74343; font-weight:bolder;" class="ui-corner-all tr borderGroove">
						<div class="td w70pr" style="padding:2%;color:White; font-weight:bolder;"><span class="inLine">Nebenaudojama nuo {{App.listAllController.endDate}}</span></div>
						<div class="td w25pr" style="text-align:center;padding:2%;"><a href="#" class="inLine" style="color:White; border:none;">Keisti</a></div>
					</div>
		{{else}}
					<div class="ui-corner-all tr borderGroove">
						<div class="td w70pr" style="padding:2%;"><input type="text" id="dialogEndDateInput" style="width:100%;" placeholder="Nebedirba nuo.."/></div>
						<div class="td w25pr" style="text-align:center;padding:2%;"><a href="#" class="inLine">Išsaugoti</a></div>
					</div>
		{{/if}}
		{{/if}}      
		</div>
</div>
