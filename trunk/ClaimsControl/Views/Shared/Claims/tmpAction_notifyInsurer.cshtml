{{#if notEditable}}
	<h2 class="tableTitle marginLeft0">{{subject}}</h2>
	<hr class="borderBottomDotted boxForDialog"/>
	<div class="row marginBottom0">
		<span class="first"><span class="comment">Įvedė: </span>{{unbound userName}}, {{entryDate}}</span>
	</div>
	<div class="row marginBottom0">
		<span><span class="comment">Pranešimo laikas: </span>{{unbound date}}</span>
	</div>
	{{view App.FormBottomView target="App.tabClaimsRegulationController"}}
{{else}}
{{log this}}
	<div class="row">	
	<div class='ExtendIt col seven date-time-select' data-ctrl='{"Field":"Date","Value":"{{unbound date}}","labelType":"Left","colLabels":"Date_forNotifyInsurer","Type":"DateTimeLess","Default":"Today","classes":{"input":"w70prImportant"},"Validity":"require().match(\"date\").lessThanOrEqualTo(new Date()).greaterThanOrEqualTo(new Date(\"{{unbound App.tabClaimsRegulationController.claim.date}}\"),[\"Data turi būt didesnė arba lygi įvykio datai\"])","Plugin":{"datepicker":{"minDate":"-3y","maxDate":"0"}}}'></div>
	</div>	
	<div class="row">	
		{{updatableField subject colLabels="Subject_forNotifyInsurer" labelType="Left" classes="wrapper:row, span:'col three', input:'col nine'"}}
	</div>	
	{{view App.FormBottomView target="App.tabClaimsRegulationController"}}
{{/if}}