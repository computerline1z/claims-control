<div class="container pad14 border-bot-lgray clearfix">

	<div class="pull-left">
		{{view App.SearchField controller="claimsController"}}
	</div>

	<div class="pull-right">
		<a class="medium" {{action "showNotDefinedEmails"  target="view"}}>Yra gautų el. laiškų kurie nepriskirti žaloms</a>
	</div>

</div><!-- container -->

{{#if App.claimsController.content.length}}
	<h2>Paskutinės žalos:</h2>
	<div class="zebra-striped selectable tall">
	{{#each "App.claimsController.content"}}
		{{#if this.visible}}
			{{#compare claimTypeID "!==" "6"}}
				<div {{action viewClaims this}} class="tr accident">
					{{view App.ClaimView}}
				</div>
			{{/compare}}
		{{/if}}
	{{/each}}
	</div>
{{else}}
	<h4>Nėra įvesta žalų..</h4>
{{/if}}
