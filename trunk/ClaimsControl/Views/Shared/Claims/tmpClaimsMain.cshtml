<div class="container">
	<div class="left big w45pr">
		{{view App.SearchField controller="claimsController"}}
	</div>
	<div class="right big"><a  class="medium" {{action "showNotDefinedEmails"  target="view"}}>Yra gautų el. laiškų kurie nepriskirti žaloms</a></div>
</div>
{{#if App.claimsController.content.length}}
	<h2 class="formTitle">Paskutinės žalos:</h2>
	<div class="zebra-striped selectable tall">
	{{#each "App.claimsController.content"}}
		{{#if this.visible}}
			{{#compare claimTypeID "!==" "6"}}
				<div {{action claimRegulation this}} class="tr accident">
					{{view App.ClaimView}}
				</div>
			{{/compare}}
		{{/if}}
	{{/each}}
	</div>
{{else}}
	<h4>Nėra įvesta žalų..</h4>
{{/if}}
