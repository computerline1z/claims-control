<table id="tblClaims">
	{{#each view.content}}
	<tr {{bindAttr class="finished:claimOK"}} {{action tbodyClick this}})>
		{{#if finished}}
			<td class="td"><i class="img32-check"></i></td>
		{{else}}
			<td class="td">&nbsp; </td>
		{{/if}}
		<td>{{no}}</td>
		<td>{{type}}</td>
		<td>{{autoNo}}</td>
		<td>{{insurer}}</td>
		{{#if totalLoss}}<td>{{totalLoss}} {{unbound App.userData.currency}}</td>{{else}}<td>{{loss}} {{unbound App.userData.currency}}</td>{{/if}}
		<td><a {{action toClaimRegulation this}}>Reguliavimas</a></td>
	</tr>
	{{/each}}
</table>
<div class="tr newClaim">
	<div class="rightFooterBig"><button {{action newClaim this}} class="btn"><i class="img18-plus mr5"></i>Nauja žala</button></div>
</div>