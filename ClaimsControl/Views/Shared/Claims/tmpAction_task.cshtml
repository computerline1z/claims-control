{{#if notEditable}}
	<h2 class="tableTitle marginLeft0">{{subject}}</h2>
	<hr class="borderBottom boxForDialog"/>
	<div class="row marginBottom0">
		<span class="first col four"><span class="comment">Įvedė: </span>{{unbound userName}}, {{entryDate}}</span>
		<span class="col four"><span class="comment">Kam: </span>{{unbound to}}</span>
		<span class="col two"><span class="comment">Atlikti iki: </span>{{unbound date}}</span>
		<label class="last col two" title="Pažymėti užduotį (ne)įvykdyta"><span class="pull-right">Atlikta {{view Em.Checkbox checkedBinding="App.tabClaimsRegulationController.taskComplete"}}</span></label>
		@*	<label class="last col two" title="Pažymėti užduotį (ne)įvykdyta"><span class="pull-right">Atlikta<input type="checkbox" {{action fnTaskComplete target="App.tabClaimsRegulationController"}}/></span></label>
	*@	
	</div>
	<hr class="borderDotted boxForDialog"/>
	<div class="row marginBottom0">
	{{unbound body}}
	</div>
	<hr class="borderDotted boxForDialog"/>
	{{view App.FormBottomView target="App.tabClaimsRegulationController"}}
{{else}}
	<div class="row">	
		{{updatableField toID labelType="Left" sTitle="Kam" classes="span:'col one first', input:'col three'"}}
		{{updatableField date labelType="Left" classes="span:'col one box-45pr-left', input:'col three last'"}}
	</div>	
	{{updatableField subject labelType="Left" classes="wrapper:row, span:'col one first', input:'col eleven'"}}
	{{updatableField body labelType="None" classes="wrapper:row, input:'row'"}}
	{{view App.FormBottomView target="App.tabClaimsRegulationController"}}
{{/if}}
