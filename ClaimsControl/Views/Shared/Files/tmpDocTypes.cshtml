<fieldset class="ui-corner-all fieldSetBorder w90pr centerBlock" style="padding:0 20px 10px;">
	<legend class="pad-b0 pad-hor4">Įvykio dokumentų tipai</legend>
	{{log this}}
	{{#each App.docsTypesController.docTypes_accident}}

		{{#if this.edit}} 

			<div class="clearfix docType pad-t10 js">
				<input {{bindAttr value="name" }} class="col seven" type="text">
				<div class="col last links-group">
					<button class="btn" {{action saveDocType this}}>Išsaugoti</button>
					<button class="btn btn-link" {{action cancelDocType this}}>Atšaukti</button>
				</div>
			</div>
		
		{{else}}
			<div class="docType content">
				<div style="float:left">{{name}}</div>		
				<div style="float:right">
					<a  {{action editDocType this}}><i class="img18-pencil"></i></a>
					<a  {{action deleteDocType this}}><i class="img18-delete"></i></a>
				</div>
			</div>
		{{/if}}

	{{/each}}
	{{#if this.addNewType2}}

		<div class="clearfix pad-t10 js">
			<input Placeholder="Įveskite naują tipą" type="text" data-category-id="2">

			<div class="col last links-group">
				<button class="btn small" {{action saveNewDocType}}>Išsaugoti</button>
				<button class="btn small" data-category-id="2" {{action cancelNewDocType}}>Atšaukti</button>
			</div>
		</div>
	
	{{else}}

		<a style="line-height:30px;" {{action addNewDocType}} data-category-id=2 >Pridėti dokumento tipą</a>

	{{/if}}
</fieldset>
</br>

